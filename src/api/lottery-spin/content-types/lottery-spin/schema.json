{
  "kind": "collectionType",
  "collectionName": "lottery_spins",
  "info": {
    "singularName": "lottery-spin",
    "pluralName": "lottery-spins",
    "displayName": "lottery-spin"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "prize": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lottery-prize.lottery-prize"
    },
    "amountUSDT": {
      "type": "decimal",
      "required": true
    },
    "costUSDT": {
      "type": "decimal",
      "required": true
    },
    "result": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "win",
        "lose"
      ]
    },
    "description": {
      "type": "string"
    }
  }
} 