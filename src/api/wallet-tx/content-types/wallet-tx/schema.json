{
  "kind": "collectionType",
  "collectionName": "wallet_txes",
  "info": {
    "singularName": "wallet-tx",
    "pluralName": "wallet-txes",
    "displayName": "wallet-tx"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "tx_type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "deposit",
        "usdt_withdraw",
        "subscription_buy",
        "subscription_redeem",
        "referral_reward",
        "lottery_prize",
        "admin_recharge",
        "usdt_withdraw_refund"
      ]
    },
    "direction": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "in",
        "out"
      ]
    },
    "amountUSDT": {
      "type": "decimal",
      "required": true
    },
    "amountToken": {
      "type": "decimal"
    },
    "memo": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  }
}
