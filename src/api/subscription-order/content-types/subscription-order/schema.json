{
  "kind": "collectionType",
  "collectionName": "subscription_orders",
  "info": {
    "singularName": "subscription-order",
    "pluralName": "subscription-orders",
    "displayName": "subscription-order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderNo": {
      "type": "uid",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "subscriptionOrders"
    },
    "plan": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::subscription-plan.subscription-plan",
      "inversedBy": "subscription_order"
    },
    "principalUSDT": {
      "type": "decimal",
      "required": true
    },
    "staticYieldUSDT": {
      "type": "decimal",
      "default": 0
    },
    "tokenBonusAmount": {
      "type": "decimal"
    },
    "cycleDays": {
      "type": "integer",
      "required": true
    },
    "state": {
      "type": "enumeration",
      "default": "draft",
      "enum": [
        "draft",
        "active",
        "due",
        "redeemed",
        " cancelled"
      ]
    },
    "startAt": {
      "type": "datetime",
      "required": true
    },
    "endAt": {
      "type": "datetime",
      "required": true
    },
    "spinQuotaGranted": {
      "type": "boolean",
      "default": false
    },
    "referralRewards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::referral-reward.referral-reward",
      "mappedBy": "fromOrder"
    }
  }
}
