{
  "kind": "collectionType",
  "collectionName": "usdt_withdraws",
  "info": {
    "singularName": "usdt-withdraw",
    "pluralName": "usdt-withdraws",
    "displayName": "usdt-withdraw"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "withdraws"
    },
    "amountUSDT": {
      "type": "decimal",
      "required": true
    },
    "toAddress": {
      "type": "string"
    },
    "network": {
      "type": "string"
    },
    "usdt_status": {
      "type": "enumeration",
      "required": false,
      "default": "pending",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ]
    },
    "txHash": {
      "type": "string"
    },
    "rejectReason": {
      "type": "text"
    },
    "reviewedAt": {
      "type": "datetime"
    }
  }
}
